
<table>
	<tr>
		<td>name</td>
		<td>category</td>
		<td>email</td>
	</tr>
	<% @jobseekers.each do |jobseeker| %>
	<tr>
		<td><%= link_to jobseeker.name, jobseeker_path(jobseeker.id) %></td>
		<td><%= jobseeker.category %></td>
		<td><%= jobseeker.email %></td>
		<td><%= link_to "edit entry", edit_jobseeker_path(jobseeker.id) %> </td>
		<td><%= link_to "delete jobseeker", jobseeker, confirm: "are you sure you want to delete this jobseeker?", method: :delete %> </td>
	</tr>
	<% end %>
</table>
<p> <%=link_to "Add more jobseekers", new_jobseeker_path %> </p>
<p> <%=link_to "Go to home page", root_path %> </p>

