<%= form_for(@jobseeker) do |f| %>
	<div class="field">
	 	<p>name</p> 
	 	<%= f.text_field :name%>
 	</div>
 	<div class="field">
 		<p>category</p> 
 		<%= f.text_field :category%>
 	</div>
 	<div class="field">
 		<p>email</p> 
 		<%= f.text_field :email%>
 	</div>
	<div class="actions">
		<%= f.submit %>
 	</div>
<% end %>