<table>
	<tr>
		<td>name</td>
		<td>category</td>
		<td>company</td>
		<td>email</td>
	</tr>
	<% @employees.each do |employee| %>
	<tr>
		<td><%= employee.name %></td>
		<td><%= employee.category %></td>
		<td><%= employee.company %></td>
		<td><%= employee.email %></td>
	</tr>
	<% end %>
</table>
<p> <%=link_to "Add more employees", new_employee_path %> </p>
<p> <%=link_to "Go to home page", root_path %> </p>